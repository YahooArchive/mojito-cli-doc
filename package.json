  "description": "Provide the `doc` command, to generate API docs, for `mojito-cli`",
  "main": "docs.js",
  "dependencies": {
    "mkdirp": "~0.3.5",
    "npmlog": "0.0.2",
    "rimraf": "~2.1.4",
    "yuidocjs": "~0.3.44"
